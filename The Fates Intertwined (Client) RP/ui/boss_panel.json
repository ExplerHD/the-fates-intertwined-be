{
    "namespace": "boss_panel",
    "boss_health_template": {
        "type": "panel",
        "anchor_from": "top_middle",
        "anchor_to": "top_middle",
        "size": [240, 61],
        "bindings": [
            {
                "binding_name": "(#bossName = $boss_name)",
                "binding_type": "collection",
                "binding_collection_name": "boss_bars",
                "binding_name_override": "#visible"
            }
        ]
    },

    "full_boss_health": {},

    "wither_bossbar@boss_health_template": {
        "controls": [
            {
                "wither_bossbar_text": {
                    "type": "label",
                    "text": "Wither",
                    "offset": [ 0, -13 ],
                    "font_size": "normal",
                    "font_type": "MinecraftTen"
                }
            },
            {
                "wither_bossbar_empty@wither_bar_empty": {
                    "layer": 1,
                    "size": [173, 7],
                    "keep_ratio": false
                }
            },
            {   
                "wither_bossbar_full@wither_bar_full": {
                    "layer": 2,
                    "size": [173, 7],
                    "keep_ratio": false
                }
            },
            {
                "wither_bossbar_overlay@wither_bar_overlay": {
                    "layer": 3,
                    "size": [190, 55],
                    "offset": [0, 9],
                    "keep_ratio": false
                }
            }
        ]
    },
